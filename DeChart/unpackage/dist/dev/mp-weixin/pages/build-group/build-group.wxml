<view class="container"><view class="top-bar"><view class="top-bar-left"><image class="back" src="/static/images/public/left.png" data-event-opts="{{[['tap',[['toBack',['$event']]]]]}}" bindtap="__e"></image></view><view class="top-bar-center">创建群聊</view><view class="top-bar-right"></view></view><view class="head"><image-cropper vue-id="97789084-1" src="{{tempFilePath}}" data-event-opts="{{[['^confirm',[['confirm']]],['^cancel',[['cancel']]]]}}" bind:confirm="__e" bind:cancel="__e" bind:__l="__l"></image-cropper><image class="g-icon" src="{{cropFilePath}}" data-event-opts="{{[['tap',[['upload',['$event']]]]]}}" bindtap="__e"></image><input class="group-name" placeholder="群名称" data-event-opts="{{[['input',[['__set_model',['','groupName','$event',[]]]]]]}}" value="{{groupName}}" bindinput="__e"/><view class="title">用户</view></view><view class="users" scroll-y="true"><view class="users-list"><block wx:for="{{myFriends}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="users-list-item"><view class="tag"><block wx:if="{{item.selected}}"><image src="/static/images/public/selected.png" data-event-opts="{{[['tap',[['tabSelect',['$0',index],[[['myFriends','',index]]]]]]]}}" bindtap="__e"></image></block><block wx:else><image src="/static/images/public/select.png" data-event-opts="{{[['tap',[['tabSelect',['$0',index],[[['myFriends','',index]]]]]]]}}" bindtap="__e"></image></block></view><view class="user-info"><image class="icon" src="{{item.iconPath}}"></image><text class="name">{{item.name}}</text></view></view></block></view></view><view class="bottom"><block wx:if="{{$root.g0}}"><view class="button">创建</view></block><block wx:else><view data-event-opts="{{[['tap',[['createGroup',['$event']]]]]}}" class="button btn-selected" bindtap="__e">{{"创建("+selectedTotal+")"}}</view></block></view></view>