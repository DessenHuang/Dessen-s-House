ange',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{array[index]}}</view></picker><image class="forward" src="/static/images/public/right.png"></image></view></block><block wx:else><block><block wx:if="{{user.sex===0}}"><view class="item-right">女</view></block><block wx:else><block wx:if="{{user.sex===1}}"><view class="item-right">男</view></block><block wx:else><view class="item-right">保密</view></block></block></block></block></view><view class="item"><view class="item-left"><text class="title">生日</text></view><view class="item-right"><block wx:if="{{isLoginUser}}"><picker style="width:100%;" mode="date" value="{{user.birth}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><uni-dateformat vue-id="29f35f98-3" date="{{user.birth}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat></picker></block><block wx:else><uni-dateformat vue-id="29f35f98-4" date="{{user.birth}}" format="yyyy/MM/dd" bind:__l="__l"></uni-dateformat></block><image class="forward" src="/static/images/public/right.png"></image></view></view><view data-event-opts="{{[['tap',[['modifyMsg',['电话','$0'],['user.phone']]]]]}}" class="item" bindtap="__e"><view class="item-left"><text class="title">电话</text></view><view class="item-right"><text class="content">{{user.phone}}</text><image class="forward" src="/static/images/public/right.png"></image></view></view><view data-event-opts="{{[['tap',[['modifyMsg',['邮箱','$0'],['user.email']]]]]}}" class="item" bindtap="__e"><view class="item-left"><text class="title">邮箱</text></view><view class="item-right"><text class="content">{{user.email}}</text><image class="forward" src="/static/images/public/right.png"></image></view></view><block wx:if="{{isLoginUser}}"><view data-event-opts="{{[['tap',[['modifyMsg',['修改密码']]]]]}}" class="item" bindtap="__e"><view class="item-left"><text class="title">密码</text></view><view class="item-right"><text class="conte